{
    "rabbitmq": {
        "ssl": {
            "cert_chain_file": "{{ sensu_client_cert_chain_file }}",
            "private_key_file": "{{ sensu_client_key_file }}"
        },
        "host": "{{ rabbitmq_host }}",
        "port": {{ rabbitmq_port }},
        "vhost": "{{ sensu_rabbitmq_vhost }}",
        "user": "{{ sensu_rabbitmq_user }}",
        "password": "{{ sensu_rabbitmq_password }}",
        "last_updated": "{{ ansible_date_time['iso8601'] }}"
    }
}
